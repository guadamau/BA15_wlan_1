\end_layout

\begin_layout Plain Layout

(./pidstat.sh dd &) 
\end_layout

\begin_layout Plain Layout

(./top.sh dd $pid_dd &) 
\end_layout

\begin_layout Plain Layout

sleep 30 
\end_layout

\begin_layout Plain Layout

pkill dd 
\end_layout

\begin_layout Plain Layout

rm /tmp/randomfile
\end_layout

\begin_layout Plain Layout

sleep 10 
\end_layout

\begin_layout Plain Layout

(iperf3 -c 192.168.0.2 -d -n 200M &) 
\end_layout

\begin_layout Plain Layout

(./meas.sh iperf3 &) 
\end_layout

\begin_layout Plain Layout

(./pidstat.sh iperf3 &) 
\end_layout

\begin_layout Plain Layout

(./top.sh iperf3 &) 
\end_layout

